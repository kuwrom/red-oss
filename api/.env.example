# OpenTelemetry Configuration
PROMETHEUS_PORT=8001

# WebSocket Configuration  
WS_HEARTBEAT_INTERVAL=30

# PII Redaction
PII_REDACTION_ENABLED=true

# Google Cloud (for Vertex AI)
GOOGLE_API_KEY=your_google_api_key_here
